@article{remennikovPredictingEffectivenessBlast2007,
  langid = {english},
  title = {Predicting the Effectiveness of Blast Wall Barriers Using Neural Networks},
  volume = {34},
  abstract = {Blast damage assessment of buildings and structural elements requires an accurate prediction of the blast loads in terms of the peak pressures and impulses. Blast loadings on structures have typically been evaluated using empirical relationships. These relationships assume that there are no obstacles between the explosive device and the target. If a blast barrier is used to protect personnel or a structure behind it, the actual blast loading environment will be signiﬁcantly reduced for some distance behind the barrier. This paper is concerned with an accurate prediction of the area of effectiveness behind the barrier using experimental data and a neural network-based model. To train and validate the neural network, a database is developed through a series of measurements of the blast environment behind the barrier. The principal parameters controlling the blast environment, such as wall height, distance behind the wall, height above ground, and standoff distance are used as the training input data. Peak overpressure and peak scaled impulse are used as the outputs in the neural network conﬁguration. The trained and validated neural network is used to develop contour plots of overpressure and impulse adjustment factors to simplify the process of predicting the effectiveness of blast barriers. The developed model is also deployed in a stand-alone application that is used as a fast-running predictive tool for the blast overpressures and impulses behind the wall.},
  number = {12},
  journaltitle = {International Journal of Impact Engineering},
  shortjournal = {International Journal of Impact Engineering},
  date = {2007-12},
  pages = {1907-1923},
  keywords = {blast wall,ML,blast},
  author = {Remennikov, Alex M. and Rose, Timothy A.},
  file = {C\:\\Users\\cip18jjp\\Google Drive\\Literature\\zotero\\Remennikov_Rose_2007_Predicting the effectiveness of blast wall barriers using neural networks.pdf}
}

@article{rigbyPredictingResponsePlates2019,
  langid = {english},
  title = {Predicting the Response of Plates Subjected to Near-Field Explosions Using an Energy Equivalent Impulse},
  volume = {128},
  abstract = {Recent experimental work by the current authors has provided highly spatially and temporally resolved measurements of the loading imparted to, and the subsequent dynamic response of, structures subjected to near-ﬁeld explosive loading [1]. In this article we validate ﬁnite element models of plates subjected to near-ﬁeld blast loads and perform a parametric study into the relationship between imparted load and peak and residual plate deformation. The energy equivalent impulse is derived, based on the theory of upper bound kinetic energy uptake introduced herein, which accounts for the additional energy imparted to a structure from a spatially non-uniform blast load. Whilst plate deﬂection is weakly correlated to total impulse, there is shown to be a strong positive correlation between deﬂection and energy equivalent impulse. The strength of this correlation is insensitive to loading distribution and mode of response. The method developed in this article has clear applications for the generation of fast-running engineering tools for the prediction of structural response to near-ﬁeld explosions.},
  journaltitle = {International Journal of Impact Engineering},
  shortjournal = {International Journal of Impact Engineering},
  date = {2019-06},
  pages = {24-36},
  author = {Rigby, S.E. and Akintaro, O.I. and Fuller, B.J. and Tyas, A. and Curry, R.J. and Langdon, G.S. and Pope, D.J.},
  file = {C\:\\Users\\cip18jjp\\Google Drive\\Literature\\zotero\\Rigby et al_2019_Predicting the response of plates subjected to near-field explosions using an energy equivalent impulse.pdf}
}

@article{rigbyNumericalInvestigationBlast2014,
  langid = {english},
  title = {A {{Numerical Investigation}} of {{Blast Loading}} and {{Clearing}} on {{Small Targets}}},
  volume = {5},
  abstract = {When a blast wave strikes a finite target, diffraction of the blast wave around the free edge causes a rarefaction clearing wave to propagate along the loaded face and relieve the pressure acting at any point it passes over. For small targets, the time taken for this clearing wave to traverse the loaded face will be small in relation to the duration of loading. Previous studies have not shown what happens in the late-time stages of clearing relief, nor the mechanism by which the cleared reflected pressure decays to approach the incident pressure. Current design guidance assumes a series of interacting clearing waves propagate over the target face – this assumption is tested in this article by using numerical analysis to evaluate the blast pressure acting on small targets subjected to blast loads. It is shown that repeat propagations of the rarefaction waves do not occur and new model is proposed, based on an over-expanded region of air in front of the loaded face of the target.},
  number = {3},
  journaltitle = {International Journal of Protective Structures},
  shortjournal = {International Journal of Protective Structures},
  date = {2014-09},
  pages = {253-274},
  author = {Rigby, Sam E. and Tyas, Andrew and Bennett, Terry and Fay, Stephen D. and Clarke, Sam D. and Warren, James A.},
  file = {C\:\\Users\\cip18jjp\\Google Drive\\Literature\\zotero\\Rigby et al_2014_A Numerical Investigation of Blast Loading and Clearing on Small Targets.pdf}
}

@article{rigbyAngleIncidenceEffects2015,
  langid = {english},
  title = {Angle of {{Incidence Effects}} on {{Far}}-{{Field Positive}} and {{Negative Phase Blast Parameters}}},
  volume = {6},
  abstract = {The blast overpressure acting on a rigid target is known to vary between the normally reflected overpressure and the incident overpressure as a function of the angle between the target and the direction of travel of the blast wave. Literature guidance for determining the exact effects of angle of incidence are unclear, particularly when considering the negative phase. This paper presents the results from a series of well controlled experiments where pressure transducers are used to record the pressure-time history acting on the face of a large, rigid target at various angles of incidence for varying sizes of hemispherical PE4 charge and stand-off distances. The test data demonstrated remarkable repeatability, and excellent agreement with semi-empirical predictions for normally reflected overpressures. The oblique results show that peak overpressure, impulse and duration are highly dependent on angle of incidence for the positive phase, and are invariant of angle of incidence for the negative phase.},
  number = {1},
  journaltitle = {International Journal of Protective Structures},
  shortjournal = {International Journal of Protective Structures},
  date = {2015-03},
  pages = {23-42},
