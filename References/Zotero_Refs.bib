  journaltitle = {Experimental Mechanics},
  shortjournal = {Exp Mech},
  date = {2019-02},
  pages = {163-178},
  author = {Rigby, S. E. and Tyas, A. and Curry, R. J. and Langdon, G. S.},
  file = {C\:\\Users\\cip18jjp\\Google Drive\\Literature\\zotero\\Rigby et al_2019_Experimental Measurement of Specific Impulse Distribution and Transient Deformation of Plates Subjected to Near-Field Explosive Blasts.pdf}
